class Solution {
    public int xorAllNums(int[] nums1, int[] nums2) {
        int ans =0;
        int n=nums1.length;
        if(n%2!=0){
            for(int a:nums2) ans^=a;
        }
        int m = nums2.length;
        if(m%2!=0){
            for(int a:nums1) ans^=a;
        }
        return ans;
    }
}
